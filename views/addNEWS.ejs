            <script>
                $("#addNEWS").on("submit", function(event) {

                    event.preventDefault();
                    var form = $('#addNEWS'); // hne t7ot id formulaire mte3ek
                    var btn = $('#btnAddNEWS'); // hne id button
                    console.log($('#addNEWS').serialize()+"&adminID=<%=user.adminID%>");
                    if($("#title").val().length>0 && $("#content").val().length>0){
                    btn.text("Ajout en cours...");
                    $.ajax({
                        type: "POST",
                        url: "http://localhost:8081/insertNews",
                        cache: false,
                        data: $('#addNEWS').serialize()+"&adminID=<%=user.adminID%>",
                        success: function(data) {
                            form.trigger('reset');
                            var obj = JSON.parse(data);
                            console.log(obj);
                            if (obj.success == true) {
                                btn.text("success").delay(2000).queue(function(next) {
                                    $(this).text("Ajouter Nouveauté");
                                    next();
                                });

                            } else {
                                btn.text("failed").delay(2000).queue(function(next) {
                                    $(this).text("Ajouter Nouveauté");
                                    next();
                                });
                            }
                        },
                        error: function(data) {
                            console.log(data);
                            btn.text("Error occured").delay(2000).queue(function(next) {
                                $(this).text("Ajouter Nouveauté");
                                next();
                            });
                            form.removeClass('has-success').addClass('has-error').delay(2000).queue(function(next) {
                                $(this).removeClass("has-error");
                                next();
                            });
                        }
                    });
                  }
                    else {
                      Materialize.toast('<span>Remplir les champs</span>', 5000);

                    }
                    $('#button').fadeIn("fast", function() {
                        $(this).delay(1000).fadeOut("slow");
                    });

                });
            </script>
